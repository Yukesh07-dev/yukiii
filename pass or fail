S=["ANU","RAJ","RAM","ANI","BALU","ROSE","BEN","TOM","SAM","ROY"]
M=[[22,33,98],[45,77,29],[89,87,12],[35,65,29],[55,44,33],[66,22,8],[88,22,13],
[99,27,56],[77,38,56],[68,63,69]]
A=["false","false","true","false","false","false","true","false","false","true"]
t=[(sum(i)//3) for i in M]
for i in range(len(S)):
    if(t[i]>=40 and A[i]=="false"):
        print(S[i],":",t[i],"% no arrear" )
